<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.novaember.xedroid"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:allowBackup="true"
            android:name="com.novaember.xedroid.Xedroid"
            android:icon="@drawable/ic_launcher"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" >
        <activity
                android:name=".OrganisationsActivity"
                android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
                android:name=".LocationsActivity"
                android:label="replace meee"
                android:parentActivityName=".OrganisationsActivity" >
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.novaember.xedroid.OrganisationsActivity" />
        </activity>

        <activity
                android:name=".AttendeesActivity"
                android:label="replace meee"
                android:parentActivityName=".LocationsActivity" >
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.novaember.xedroid.LocationsActivity" />
        </activity>

        <activity
                android:name=".WeekScheduleActivity"
                android:label="replace meee"
                android:parentActivityName=".WeekScheduleActivity" >
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.novaember.xedroid.AttendeesActivity" />
        </activity>
    </application>

</manifest>
